{
	"name": "slevomat/async-smtp-mailer",
	"description": "Asynchronous SMPT mailer",
	"homepage": "http://github.com/slevomat/async-smtp-mailer",
	"license": ["proprietary"],
	"authors": [
		{
			"name": "Slevomat.cz, s.r.o.",
			"email": "it@slevomat.cz",
			"homepage": "http://www.slevomat.cz"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php-64bit": ">=7.2.2",
		"consistence-php/consistence": "dev-master#d28c63d",
		"consistence/consistence": "1.0.2",
		"nette/mail": "2.4.4",
		"react/event-loop": "0.4.3",
		"react/promise": "2.5.1",
		"react/socket": "^0.8.10"
	},
	"require-dev": {
		"consistence/coding-standard": "2.3.1",
		"jakub-onderka/php-console-highlighter": "0.3.2",
		"jakub-onderka/php-parallel-lint": "1.0.0",
		"phpstan/phpstan": "0.9.2",
		"phpstan/phpstan-dibi": "0.9",
		"phpstan/phpstan-doctrine": "0.9.1",
		"phpstan/phpstan-nette": "0.9",
		"phpstan/phpstan-phpunit": "0.9.4",
		"phpstan/phpstan-sentry": "0.9",
		"phpstan/phpstan-strict-rules": "0.9",
		"phpunit/phpunit": "7.0.2",
		"slevomat/coding-standard": "4.5.2",
		"slevomat/deptrac-shim": "dev-master#3d00766",
		"spell-checker/dictionary-cs": "0.2.2",
		"spell-checker/dictionary-en": "0.1.1",
		"spell-checker/dictionary-sk": "0.2.2",
		"spell-checker/spell-checker": "0.3.1",
		"squizlabs/php_codesniffer": "3.2.3"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:slevomat/consistence.git"
		}
	],
	"config": {
		"bin-dir": "bin"
	},
	"autoload": {
		"classmap": ["app/src", "tests", "tests-examples"]
	}
}
