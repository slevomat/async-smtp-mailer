{
	"name": "slevomat/async-smtp-mailer",
	"description": "Asynchronous SMTP mailer",
	"homepage": "http://github.com/slevomat/async-smtp-mailer",
	"license": ["proprietary"],
	"authors": [
		{
			"name": "Slevomat.cz, s.r.o.",
			"email": "it@slevomat.cz",
			"homepage": "http://www.slevomat.cz"
		}
	],
	"require": {
		"php": "^7.4.0 || ^8.0.0",
		"nette/di": "^3.0.7",
		"nette/mail": "^3.1.5",
		"psr/log": "^1.1.3",
		"react/event-loop": "^1.1.1",
		"react/promise": "^2.8.0",
		"react/socket": "^1.6.0",
		"slevomat/consistence": "^2.1.0"
	},
	"require-dev": {
		"php-parallel-lint/php-parallel-lint": "^1.3.2",
		"phing/phing": "^2.17.3",
		"phpstan/phpstan": "^1.6.8",
		"phpstan/phpstan-phpunit": "^1.1.1",
		"phpstan/phpstan-strict-rules": "^1.2.3",
		"phpunit/phpunit": "^9.5.20",
		"slevomat/coding-standard": "^7.2.0",
		"spell-checker/spell-checker": "v0.3.22",
		"spell-checker/dictionary-en": "v0.1.1",
		"squizlabs/php_codesniffer": "^3.6.2"
	},
	"config": {
		"bin-dir": "bin",
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"autoload": {
		"psr-4": {"AsyncConnection\\": ["src/"]}
	},
	"autoload-dev": {
		"classmap": ["tests"]
	},
	"prefer-stable": true
}
